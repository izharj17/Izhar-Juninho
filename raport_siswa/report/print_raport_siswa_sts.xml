<?xml version="1.0"?>
<odoo>
    <template id="print_raport_siswa_sts">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o">
                <div class="page">
                    <h1>Raport Siswa STS</h1>
                    <table class="table table-condensed">
                        <thead>
                            <tr>
                                <th>Raport ID</th>
                                <th>Jenis Raport</th>
                                <th>Student</th>
                                <th>Kelas</th>
                                <th>Semester</th>
                                <th>Tahun Pelajaran</th>
                                <th>State</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td><span t-field="o.kode_seq"/></td>
                                <td><span t-field="o.jenis_raport"/></td>
                                <td><span t-field="o.student_id.name"/></td>
                                <td><span t-field="o.kelas_id.name"/></td>
                                <td><span t-field="o.semester_id"/></td>
                                <td><span t-field="o.tahun_pelajaran.name"/></td>
                                <td><span t-field="o.state"/></td>
                            </tr>
                        </tbody>
                    </table>
                    <!-- Add more sections or details as needed -->
                </div>
            </t>
        </t>
    </template>
</odoo>
