<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_master_tipe_penerima_tree" model="ir.ui.view">
            <field name="name">master.tipe.penerima.tree</field>
            <field name="model">master.tipe.penerima</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <tree string="Master Tipe Penerima">
                    <field name="nama_tipe_penerima" string="Nama Tipe Penerima"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>

        <record id="view_master_tipe_penerima_form" model="ir.ui.view">
            <field name="name">master.tipe.penerima.form</field>
            <field name="model">master.tipe.penerima</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <form string="Master Tipe Penerima">
                    <sheet>
                        <group col="4">
                            <field name="nama_tipe_penerima"/>
                            <field name="code"/>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                        <field name="message_ids" widget="mail_thread"/>
                        <field name="activity_ids" widget="mail_activity"/>
                    </div>
                </form>
            </field>
        </record>

        <record id="view_master_tipe_penerima_search" model="ir.ui.view">
            <field name="name">master.tipe.penerima.search</field>
            <field name="model">master.tipe.penerima</field>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <search string="Master Tipe Penerima">
                    <field name="nama_tipe_penerima"/>
                    <field name="code"/>
                    <newline/>
                    <group expand="0" string="Group By...">
                        <filter string="Tipe Penerima" name="group_by_master_tipe_penerima_nama_tipe_penerima"
                                domain="[]" context="{'group_by':'nama_tipe_penerima'}"/>
                        <filter string="Code" name="group_by_master_tipe_penerima_code"
                                domain="[]" context="{'group_by':'code'}"/>
<!--                        <filter string="Archived" name="inactive" domain="[('active', '=', False)]"/>-->
                    </group>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_master_tipe_penerima_view">
            <field name="name">Master Tipe Penerima</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">master.tipe.penerima</field>
            <field name="binding_view_types">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_master_tipe_penerima_search"/>
        </record>

        <record model="ir.actions.act_window.view" id="act_master_tipe_penerima_view_form">
            <field name="act_window_id" ref="act_master_tipe_penerima_view"/>
            <field name="sequence" eval="20"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_master_tipe_penerima_form"/>
        </record>

        <record model="ir.actions.act_window.view" id="act_master_tipe_penerima_view_tree">
            <field name="act_window_id" ref="act_master_tipe_penerima_view"/>
            <field name="sequence" eval="10"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_master_tipe_penerima_tree"/>
        </record>

        <menuitem id="menu_master_tipe_penerima_details"
                    name="Tipe Penerima"
                    parent="menu_master_donasi_root"
                    sequence="50"
                    action="act_master_tipe_penerima_view"/>

    </data>
</odoo>